<div id="gallery">
  <h1 class="headingStyle" style="margin-top:100px;color:#071f6b;">Gallery</h1>
  <div class="container">
    <div class="gallery" id="gallery1">
      <div id="carouselExampleControlsGallery" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <div class="images">
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="images/gallery_1.jpg">
              </div>
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="images/gallery_2.jpg">              
              </div>
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="images/gallery_3.jpg">
              </div>
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="images/gallery_4.jpg">              
              </div>
            </div>
          </div> 
          <!-------------------------------image-->
          <% if(items.length>0) { %>
          <% items.forEach(e => {  %>
          <div class="carousel-item">
            <div class="images">
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="uploads/<%= e.file1.filename %>">
              </div>
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="uploads/<%= e.file2.filename %>">              
              </div>
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="uploads/<%= e.file3.filename %>">
              </div>
              <div class="gallery-item">
                <div class="image-overlay"></div>
                <img src="uploads/<%= e.file4.filename %>">              
              </div>
            </div>
          </div> 

          <% }) %>
        <% } %>

          <!-------------------------------image-->  
        </div>
        <div class="arrows">
          <a class="carousel-control-prev" href="#carouselExampleControlsGallery" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleControlsGallery" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
    </div>
    <div class="awards">
      <h3>Awards</h3>
      <div id="carouselExampleControlsAwards" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-block w-50 mx-auto" src="images/ek.png" alt="First slide">
            <p>
              We envision to grow as an idea across regions, economies and countries and guide 
              people to change their lives for not only their betterment but the overall development 
              of the society. 
            </p>
          </div>
          <div class="carousel-item">
            <img class="d-block w-50 mx-auto" src="images/circle.png" alt="Second slide">
            <p>
              We envision to grow as an idea across regions, economies and countries and guide 
              people to change their lives for not only their betterment but the overall development 
              of the society.
            </p>
          </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControlsAwards" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControlsAwards" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>
  </div>
</div>